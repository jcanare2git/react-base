urnal/NothingSelected.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/components/notes/NoteScreen.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/components/notes/NotesAppBar.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/store/store.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/reducers/authReducer.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/types/types.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/hooks/useForm.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/actions/auth.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/firebase/firebaseConfig.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/reducers/uiReducer.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/actions/ui.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/routers/AppRouter.js",["100"],"import React, { useEffect, useState } from 'react'\nimport {\n    BrowserRouter as Router,\n    Switch, Route, Redirect\n} from \"react-router-dom\";\n\nimport { useDispatch } from 'react-redux';\n\nimport { JournalScreen } from '../components/journal/JournalScreen';\nimport { AuthRouter } from './AuthRouter';\nimport { PrivateRoutes } from './PrivateRoutes';\nimport { PublicRoutes } from './PublicRoutes';\n\nimport { firebase } from '../firebase/firebaseConfig';\nimport { login } from '../actions/auth';\n\nexport const AppRouter = () => {\n\n\n    const dispatch = useDispatch();\n\n    //Flag para esperar respuesta desde Firebase\n    const [checking, setChecking] = useState(true);\n\n    //Flag para verificar que esté autenticado\n    const [isLoggedIn, setIsLoggedIn] = useState(false);\n\n    //Para verificar que el usuario esté logueado\n    useEffect(() => {\n\n        firebase.auth().onAuthStateChanged((user) => {\n\n            if (user?.uid) {\n                dispatch(login(user.uid, user.displayName));\n                setIsLoggedIn(true);\n            } else {\n                setIsLoggedIn(false);\n            }\n\n            //Ya culminó chequeo\n            setChecking(false);\n\n        });\n\n    }, [dispatch, setChecking, setIsLoggedIn])\n\n    if (checking) {\n        return (\n            <h1>Espere...</h1>\n        )\n    }\n\n    return (\n        <Router>\n            <div>\n                <Switch>\n                    <PublicRoutes\n                        path=\"/auth\"\n                        component={AuthRouter}\n                        isAuthenticated= {isLoggedIn}\n                    />\n\n                    <PrivateRoutes\n                        exact\n                        path=\"/\"\n                        component={JournalScreen}\n                        isAuthenticated= {isLoggedIn}\n                    />\n\n                    <Redirect to=\"/auth/login\" />\n                </Switch>\n            </div>\n        </Router>\n    )\n}\n","/home/juan/Escritorio/Cursos/React/08-journal-app/src/routers/AuthRouter.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/routers/PrivateRoutes.js",[],"/home/juan/Escritorio/Cursos/React/08-journal-app/src/routers/PublicRoutes.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":3,"column":10,"nodeType":"107","messageId":"108","endLine":3,"endColumn":25},{"ruleId":"105","severity":1,"message":"109","line":4,"column":13,"nodeType":"107","messageId":"108","endLine":4,"endColumn":18},"no-native-reassign",["110"],"no-negated-in-lhs",["111"],"no-unused-vars","'NothingSelected' is defined but never used.","Identifier","unusedVar","'Route' is defined but never used.","no-global-assign","no-unsafe-negation"]